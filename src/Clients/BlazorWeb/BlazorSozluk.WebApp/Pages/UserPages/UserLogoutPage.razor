@page "/logout"

<h3>logout</h3>

@code {
    [Inject] IIdentityService _identityService { get; set; }
    [Inject] NavigationManager _navigationManager { get; set; }

    protected override void OnInitialized()
    {
        _identityService.Logout();
        _navigationManager.NavigateTo("/");
    }
  
}
